<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/walp4"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/staButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imageView1asd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/adrod"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView7asd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Sta creator"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/scriptButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imageVieasd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="0.5"
                android:scaleY="0.5"
                android:src="@drawable/folder"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textw7asd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Script Toolbox"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/dumpModemButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imasieasd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="1.1"
                android:scaleY="1.1"
                android:src="@drawable/ic_modem"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/te7asd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Dump Modem"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/armButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imasieasagrd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/ic_sensor"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tegggsdfsd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Arm software"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/atlasosButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imasisagrd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/atlasos"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tegggfsd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Copy AtlasOS/ReviOS files"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/dbkpButton"
            android:layout_width="match_parent"
            android:layout_height="200px"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="50px"
            android:background="@drawable/rounded_light_blue"
            android:clickable="true"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="PxUsage">

            <ImageView
                android:id="@+id/imaasdsisagrd"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:scaleX="0.9"
                android:scaleY="0.9"
                android:src="@drawable/ic_uefi"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tasdegggfsd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Dualboot kernel patcher"
                android:textColor="@color/light_blue"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>